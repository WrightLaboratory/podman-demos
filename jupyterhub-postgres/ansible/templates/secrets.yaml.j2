apiVersion: v1
kind: Secret
metadata:
  name: github-oauth-secrets
type: Opaque
data:
  client_id: {{ gh_client_id | b64encode }}
  client_secret: {{ gh_client_secret | b64encode }}
  callback_url: {{ gh_oauth_callback_url | b64encode }}
